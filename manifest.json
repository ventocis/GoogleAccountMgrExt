{
    "background": {
       "persistent": true,
       "scripts": [ "content.js" ]
    },
    "browser_action": {
       "default_popup": "popup.html"
    },
    "manifest_version": 2,
    "name": "Google Services Shortcuts",
    "permissions": [ "contextMenus", "tabs", "storage" ],
    "update_url": "https://clients2.google.com/service/update2/crx",
    "version": "0.8.2",
    "icons": {
      "128": "128pxLogo3",
      "48": "48pxLogo3.png",
      "16": "16pxLogo3.png"
    }
 }